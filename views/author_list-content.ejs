<h1><%=title%></h1>
<ul>
    <% if(author_list){ %> <% for(const author of author_list) {%>
    <li>
        <a href="<%=author.url%>"><%=author.name%></a> (
        <%=author.date_of_birth_formatted%> -
        <%=author.date_of_death_formatted%> )
    </li>
    <% } %> <% } else{ %>
    <p>There are no authors</p>

    <% } %>
</ul>
