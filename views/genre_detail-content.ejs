<h1>Genre: <%=genre.name%></h1>
<div style="margin-left: 20px; margin-top: 20px">
    <h4>Books</h4>
</div>
<ul>
    <% if(genre_books.length > 0){ %> <% for(const book of genre_books) {%>
    <li>
        <strong><a href="<%=book.url%>"><%=book.title%></a></strong>
    </li>
    <section><%=book.summary%></section>

    <% } %>
</ul>
<% } else { %>
<p>This genre has no books</p>

<% } %>

<hr />
<p><a href="<%= genre.url %>/delete ">Delete Genre</a></p>
